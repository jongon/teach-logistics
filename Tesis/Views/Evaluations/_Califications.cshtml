@model IEnumerable<Tesis.ViewModels.CalificationVieWModel>
<table id="califications" class="table table-striped table-bordered">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.FirstName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.LastName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.SemesterDescription)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.SectionNumber)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.GotScore)
			</th>
			<th>
				Acción
			</th>
		</tr>
	</thead>
	<tfoot>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.FirstName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.LastName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.SemesterDescription)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.SectionNumber)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.GotScore)
			</th>
			<th>
				Acción
			</th>
		</tr>
	</tfoot>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.FirstName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.LastName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.SemesterDescription)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.SectionNumber)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.GotScore)
				</td>
				<td>
					@Html.ActionLink("Revisar Quiz", "ReviewQuiz", new { userId = item.UserId, evaluationId = item.EvaluationId }, new { @class = "btn btn-primary btn-sm" })		
				</td>
			</tr>
		}
	</tbody>
</table>