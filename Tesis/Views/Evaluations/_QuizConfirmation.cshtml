<div class="modal inmodal" id="quizModal" tabindex="-1" role="dialog" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content animated fadeIn">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Cerrar</span></button>
				<i class="fa fa-question modal-icon"></i>
				<h4 class="modal-title">Comenzar Quiz</h4>
			</div>
			<div class="modal-body">
				<p>
					<strong>Instrucciones del Quiz:</strong>Preguntas de selección simple, solo se podrá selccionar una opcion, ¡ÉXITO!
				</p>
			</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-white" data-dismiss="modal">Cerrar</button>
					@Html.ActionLink("Presentar Quiz", "TakeQuiz", new { id = "" }, new { @class = "btn btn-primary btn-sm", @id = "takeQuiz" })
				</div>
		</div>
	</div>
</div>