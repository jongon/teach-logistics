@model IList<Tesis.ViewModels.ProductDemand>
@for (int i = 0; i < Model.Count(); i++ )
{
	<div class="form-group">
		@Html.Label(Model[i].Product.Name, new { @class = "control-label col-md-2" })
		<div class="col-md-10">
			@Html.HiddenFor(model => model[i].Product.Id)
			@Html.EditorFor(model => model[i].Quantity, new { htmlAttributes = new { @placeholder = "Cantidad Vendida", @Value = "" } })
			@Html.ValidationMessageFor(model => model[i].Quantity, "", new { @class = "text-danger" })
		</div>
	</div>
}